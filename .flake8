[flake8]
extend-ignore = E203, E265,E266, E501, W503, D210, D212, F405, F403, C901, W605
max-line-length = 88
max-complexity = 18
exclude =
    .git
    __pycache__
    build
    dist
    releases
    .venv
    venv
    .tox
    .mypy_cache
    .pytest_cache
    .vscode
    .coverage
    *.egg-info
verbose = 1
statistics = True